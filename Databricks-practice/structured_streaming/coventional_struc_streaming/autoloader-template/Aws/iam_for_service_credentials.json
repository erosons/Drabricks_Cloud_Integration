{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::redshift-stagingarea/*",
                "arn:aws:s3:::redshift-stagingarea"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "sts:AssumeRole"
            ],
            "Resource": [
                "arn:aws:iam::<WSAcountID>:role/autoloader_servicec_credentials"
            ],
            "Effect": "Allow"
        }
    ]
}